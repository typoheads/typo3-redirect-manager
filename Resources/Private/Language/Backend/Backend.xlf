    <file source-language="en" datatype="plaintext" original="messages" date="2021-02-10T09:00:00Z" product-name="redirect_manager">
        <header/>
        <body>
            <!--
                Extension configuration.
            -->
            <trans-unit id="configuration.pageErrorHandlingResponseType" xml:space="preserve">
                <source>What response to produce, after the custom 404 error-handler was executed.</source>
            </trans-unit>
            <trans-unit id="configuration.pageErrorHandlingResponseType.default" xml:space="preserve">
                <source>(Default) Imitate default ErrorPageController.</source>
            </trans-unit>
            <trans-unit id="configuration.pageErrorHandlingResponseType.url" xml:space="preserve">
                <source>Redirect to URL. The given parameter below will be interpreted as the target URL.</source>
            </trans-unit>
            <trans-unit id="configuration.pageErrorHandlingResponseType.page" xml:space="preserve">
                <source>Redirect to TYPO3 page. The given parameter below will be interpreted as a page UID.</source>
            </trans-unit>
            <trans-unit id="configuration.pageErrorHandlingResponseType.class" xml:space="preserve">
                <source>Redirect via PHP-Class. The given parameter below is an ErrorHandler implementing PageErrorHandlerInterface.</source>
            </trans-unit>
            <trans-unit id="configuration.pageErrorHandlingResponseType.exception" xml:space="preserve">
                <source>Throw PageNotFoundException. Probably unsafe for production-environments.</source>
            </trans-unit>
            <trans-unit id="configuration.pageErrorHandlingResponseValue" xml:space="preserve">
                <source>The response-parameter. Meaning depends on what was previously selected as the response-type.</source>
            </trans-unit>
            <trans-unit id="configuration.enableNotFoundLogging" xml:space="preserve">
                <source>Whether to log 404-errors into a database table. This data is then used by the backend module to display possibly broken URLs.</source>
            </trans-unit>
            <trans-unit id="configuration.notFoundLogStorage" xml:space="preserve">
                <source>Storage PID where to save 404-errors. Defaults to the 0-root page.</source>
            </trans-unit>


            <!--
                Backend module.
            -->
            <!--Redirects-->
            <trans-unit id="tx_redirectmanager_not_found_log.listTitle" xml:space="preserve">
                <source>404 Not Found log entry</source>
            </trans-unit>
            <trans-unit id="tx_redirectmanager_not_found_log.url" xml:space="preserve">
                <source>URL</source>
            </trans-unit>
            <trans-unit id="tx_redirectmanager_not_found_log.hash" xml:space="preserve">
                <source>Hashed URL</source>
            </trans-unit>
            <trans-unit id="tx_redirectmanager_not_found_log.hit_count" xml:space="preserve">
                <source>Number of hits for this URL</source>
            </trans-unit>
            <trans-unit id="tx_redirectmanager_not_found_log.is_resolved" xml:space="preserve">
                <source>Whether this URL has been resolved/fixed</source>
            </trans-unit>
            <trans-unit id="tx_redirectmanager_not_found_log.is_resolved.1" xml:space="preserve">
                <source>Yes</source>
            </trans-unit>
            <trans-unit id="tx_redirectmanager_not_found_log.has_reappeared_count" xml:space="preserve">
                <source>Number of times this URL was hit after it was already resolved</source>
            </trans-unit>

            <!--Redirects->index-->
            <trans-unit id="backendModule.redirects.index.title" xml:space="preserve">
                <source>Redirects+</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.index.description" xml:space="preserve">
                <source>Provides additional tools for managing redirects.</source>
            </trans-unit>

            <!--Redirects->listNotFound-->
            <trans-unit id="backendModule.redirects.listNotFound.title" xml:space="preserve">
                <source>404 Not Found requests</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.description" xml:space="preserve">
                <source>Lists all 404 Not Found requests. With the help of this list, e.g. broken or missing redirects can be fixed by creating new redirects.</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.noEntriesFound" xml:space="preserve">
                <source>No 404 Not Found requests found.</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.status" xml:space="preserve">
                <source>Status</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.status.isResolved" xml:space="preserve">
                <source>Resolved</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.status.isResolvedButHasReappeared" xml:space="preserve">
                <source>Reappeared</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.status.isNew" xml:space="preserve">
                <source>New</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.status.hasNewHits" xml:space="preserve">
                <source>New hits</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.url" xml:space="preserve">
                <source>URL</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.hitCount" xml:space="preserve">
                <source>Hit<![CDATA[<br>]]>count</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.hasReappearedCount" xml:space="preserve">
                <source>Has-reappeared<![CDATA[<br>]]>count</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.crdate" xml:space="preserve">
                <source>Appeared at</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.actions" xml:space="preserve">
                <source>Actions</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.actions.edit" xml:space="preserve">
                <source>Edit log entry</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.actions.delete" xml:space="preserve">
                <source>Delete log entry</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.actions.createRedirect" xml:space="preserve">
                <source>Create redirect with TYPO3 core redirects extension</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.actions.resolve" xml:space="preserve">
                <source>Mark as resolved</source>
            </trans-unit>
            <trans-unit id="backendModule.redirects.listNotFound.table.actions.unresolve" xml:space="preserve">
                <source>Mark as not resolved</source>
            </trans-unit>
        </body>
